####### Compiler, tools and options

CC       = gcc
CXX      = g++
LINK     = g++
DEL_FILE = rm -rf

####### Files

HEADERS = BoolArray.h \
		GraphMeasure.h \
		RankCorr.h
SOURCES = GraphCluster.cpp \
		GraphMeasure.cpp \
		RankCorr.cpp
OBJECTS = GraphCluster.o \
		GraphMeasure.o \
		RankCorr.o

TARGET   = discretize

first: all

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(OBJECTS)
	$(LINK) $(LDFLAGS) -o $(TARGET) $(OBJECTS)

clean:
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core

distclean: clean
	-$(DEL_FILE) $(TARGET)

####### Compile

GraphCluster.o: GraphCluster.cpp GraphMeasure.h \
		BoolArray.h

GraphMeasure.o: GraphMeasure.cpp GraphMeasure.h \
		BoolArray.h

RankCorr.o: RankCorr.cpp 

####### Install

install: all  

uninstall:  

