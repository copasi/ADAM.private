Type Definition
VariableId: string match x[0-9]+ 
ParameterId: string match k[0-9]+ 
UnknownId: string match u[0-9]+ 
VariableIdRef: existing VariableId
ParameterIdRef: existing ParameterId
UnknownIdRef: existing UnknownId
State: digit | digit1-9 digits

{
  "model": {
    "name": string,
    "description": string,
    "version": string,
    "variables": [
      {
        "name": string,
        "description": string,
        "id": VariableId,
        "states": [string, ...],
        "annotations": string, 
      }
      , ....
    ]
    "parameters": [
      {
        "name": string,
        "description": string,
        "id": ParameterId,
        "states": [string, ...],
        "annotations": string, 
      }
      , ...
    ],
    "unknownObservation": {
      VariableIdRef: [UnknownId, ...]
    },
    "updateRules": {
        VariableIdRef: { 
            "possibleInputVariables": [VariableIdRef | ParameterIdRef, ...],
            "booleanExpression": "x1 OR x2",
            "polynomialFunction": "x1*x2",
            "transitionTable": {
                "rows": { 
                    [[State, ...], State | UnknownIdRef]
                    , ...
                }
            }
        }
    }
}
